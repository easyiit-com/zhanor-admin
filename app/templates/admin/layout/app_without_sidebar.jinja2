   <script>
      function _(text) {
        const translations = {
          "Logout Successfully": '{{_("Logout Successfully")}}',
          "Please wait": '{{_("Please wait")}}',
          "Delete Successfully": '{{'{{_("Delete Successfully")}}'}}',
          "Delete Faile": '{{_("Delete Faile")}}',
          "Please first select the items you want to delete": '{{_("Please first select the items you want to delete")}}',
          "Submit Successfully": '{{_("Submit Successfully")}}',
          "Upload Successfully": '{{_("Upload Successfully")}}',
          "Failed to upload files.": '{{_("Failed to upload files.")}}',
          "Copy Successfully": '{{_("Copy Successfully")}}',
          "Copy Failed": '{{_("Copy Failed")}}'
        };
        return translations[text] || text; // 如果没有找到对应的翻译，则返回原文本
      }
    </script>
